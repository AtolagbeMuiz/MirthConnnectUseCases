tmp[&apos;MSH&apos;][&apos;ReceivingApplication&apos;] = msg[&apos;MSH&apos;][&apos;MSH.5&apos;][&apos;MSH.5.1&apos;].toString()

tmp[&apos;MSH&apos;][&apos;ReceivingFacility&apos;] = msg[&apos;MSH&apos;][&apos;MSH.6&apos;][&apos;MSH.6.1&apos;].toString()

tmp[&apos;MSH&apos;][&apos;MessageType&apos;][&apos;MessageCode&apos;] = msg[&apos;MSH&apos;][&apos;MSH.9&apos;][&apos;MSH.9.1&apos;].toString()

tmp[&apos;MSH&apos;][&apos;MessageType&apos;][&apos;TriggerEvent&apos;] = msg[&apos;MSH&apos;][&apos;MSH.9&apos;][&apos;MSH.9.2&apos;].toString()

//EVN Segment Transformer
tmp[&apos;EVN&apos;][&apos;EventTypeCode&apos;] = msg[&apos;EVN&apos;][&apos;EVN.1&apos;][&apos;EVN.1.1&apos;].toString()

tmp[&apos;EVN&apos;][&apos;RecordedDateTime&apos;] = msg[&apos;EVN&apos;][&apos;EVN.3&apos;][&apos;EVN.3.1&apos;].toString()

//PID segment Transformer
tmp[&apos;PID&apos;][&apos;PatientName&apos;][&apos;FamilyName&apos;] = msg[&apos;PID&apos;][&apos;PID.5&apos;][&apos;PID.5.1&apos;].toString()

tmp[&apos;PID&apos;][&apos;PatientName&apos;][&apos;GivenName&apos;] = msg[&apos;PID&apos;][&apos;PID.5&apos;][&apos;PID.5.2&apos;].toString()

tmp[&apos;PID&apos;][&apos;DateTimeOfBirth&apos;] = msg[&apos;PID&apos;][&apos;PID.7&apos;][&apos;PID.7.1&apos;].toString()

tmp[&apos;PID&apos;][&apos;AdministrativeSex&apos;] = msg[&apos;PID&apos;][&apos;PID.8&apos;][&apos;PID.8.1&apos;].toString()

tmp[&apos;PID&apos;][&apos;PatientAddress&apos;][&apos;StreetAddress&apos;] = msg[&apos;PID&apos;][&apos;PID.11&apos;][&apos;PID.11.1&apos;].toString() + &quot; &quot; + msg[&apos;PID&apos;][&apos;PID.11&apos;][&apos;PID.11.3&apos;].toString() + &quot; &quot; + msg[&apos;PID&apos;][&apos;PID.11&apos;][&apos;PID.11.4&apos;].toString() + &quot; &quot; + msg[&apos;PID&apos;][&apos;PID.11&apos;][&apos;PID.11.5&apos;].toString()

tmp[&apos;PID&apos;][&apos;EmailAddress&apos;] = msg[&apos;PID&apos;][&apos;PID.13&apos;][&apos;PID.13.4&apos;].toString()

